let parteSalario = Salario / 5
if ( parteSalario < Prestacao) console.log ("emprestimo nao concedido")
else (console.log("Emprestimo concedido"))


let numero = Number (prompt("insira o numero"))
switch(numero){
    case 1: 
    console.log("domingo") 
    break
    case 2:
    console.log("segunda feira")
    break
    case 3:
    console.log("terca feira")
    break
    case 4:
    console.log("quarta feira") 
    break
    case 5:
    console.log("quinta feira") 
    break
    case 6:
    console.log("sexta feira") 
    break
    case 7:
    console.log("sabado ") 
    break
    default:
    console.log("invalido")
}


