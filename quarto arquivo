            let gols2 = Number(prompt("numero de gols do segundo tempo"))
            if (gols1 == gols2 )console.log (" empate")
            else if (gols2 < gols1 )console.log ("vitoria do primeiro tempo")
            else (console.log("vitoria do segundo tempo"))

