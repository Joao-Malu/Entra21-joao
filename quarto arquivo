        let lado1 = Number(prompt("digite o numero"))
        let lado2 = Number(prompt("digite o numero"))
        let lado3 = Number(prompt("digite o numero"))
        if (lado1 == lado2 && lado2 == lado3) console.log ("triangulo equilatero") 
        else if (lado1 == lado2 || lado2 == lado3 || lado3 == lado1) console.log ("triangulo isoceles")
        else (console.log ("triangulo escaleno"))

