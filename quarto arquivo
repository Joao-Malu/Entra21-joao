    let numero1 = Number(prompt("digite o numero"))
    let numero2 = Number(prompt("digite o numero"))
    let numero3 = Number(prompt("digite o numero"))
    if (numero1 >= numero2 && numero1 >=numero3) console.log(`${numero1} e maior numero`)
    if (numero2 > numero1 &&  numero2 >= numero3) console.log(`${numero2} e maior numero`)
    if (numero3 > numero2 && numero3> numero1) console.log(`${numero3} e maior numero`)


